<Script>
    <![CDATA[
    /**
     * Takes RGB values (each between 0 and 255) and returns the corresponding HSL values (H between 0 and 360, S and L each between 0 and 100).
     * Based on http://stackoverflow.com/a/9493060
     */
    collection.rgbToHsl = function(r, g, b) {
        var max, min, h, s, l;

        r = r / 255;
        g = g / 255;
        b = b / 255;

        max = Math.max(r, g, b);
        min = Math.min(r, g, b);

        l = (min + max) / 2;

        diff = max - min;

        if (diff == 0) {
            s = 0;
            h = 0;
        } else {
            if(l > 0.5) {
                s = (diff) / (2 - min - max)
            } else {
                s = diff / (max + min)
            }

            switch(max) {
                case r:
                    h = (g - b) / diff + (g < b ? 6 : 0);
                    break;
                case g:
                    h = (b - r) / diff + 2;
                    break;
                case b:
                    h = (r - g) / diff + 4;
                    break;
            }
        }

        return {h: Math.round(h * 60), s: Math.round(s * 100), l: Math.round(l * 100)};
    }

    /**
     * Takes HSL values (H between 0 and 360, S and L each between 0 and 100) and returns the corresponding RGB values (each between 0 and 255)
     * Based on pseudo-code in the W3 Color Model document (http://www.w3.org/TR/2011/REC-css3-color-20110607/#hsl-color)
     */
    collection.hslToRgb = function (h, s, l) {
        var m1, m2, m3, r, g, b;

        h = h / 360;
        s = s / 100;
        l = l / 100;

        m2 = l <= 0.5 ? l * (s + 1) : l + s - l * s;

        m1 = l * 2 - m2;

        r = collection.hueToRgb(m1, m2, h + 1/3);
        g = collection.hueToRgb(m1, m2, h);
        b = collection.hueToRgb(m1, m2, h - 1/3);

        return {r: Math.round(r * 255), g: Math.round(g * 255), b: Math.round(b * 255)};
    }

    collection.hueToRgb = function (m1, m2, h) {
        if(h < 0) {
            h = h + 1;
        } else if(h > 1) {
            h = h - 1;
        }

        if(h*6 < 1) {
            return m1 + (m2 - m1) * h * 6;
        } else if(h*2 < 1) {
            return m2;
        } else if(h*3 < 2) {
            return m1 + (m2 - m1) * (2/3 - h) * 6
        }

        return m1;
    }

    /**
     * Darkens the given color by the given amount (a number between 0 and 100)
     */
    collection.darken = function (color, amount) {
        return collection._adjustLightness(color, -amount);
    }

    /**
     * Lightens the given color by the given amount (a number between 0 and 100)
     */
    collection.lighten = function (color, amount) {
        return collection._adjustLightness(color, amount);
    }

    /**
     * Offsets the hue of a color by the given amount (in degrees)
     */
    collection.spin = function (color, amount) {
        var hsl = collection.rgbToHsl(color.r, color.g, color.b);
        newH = (hsl.h + amount) % 360;
        hsl.h = (newH >= 0 ? newH : newH + 360);

        rgb = collection.hslToRgb(hsl.h, hsl.s, hsl.l);

        return Color.fromString("rgba(" + rgb.r + "," + rgb.g + "," + rgb.b + "," + color.a + ")");
    }

    /**
     * Changes the lightness of the given color by the given amount (a number between -100 and 100)
     */
    collection._adjustLightness = function (color, amount) {
        var hsl = collection.rgbToHsl(color.r, color.g, color.b);

        hsl.l = Math.min(Math.max(hsl.l + amount, 0), 100);
        rgb = collection.hslToRgb(hsl.h, hsl.s, hsl.l);

        return Color.fromString("rgba(" + rgb.r + "," + rgb.g + "," + rgb.b + "," + color.a + ")");
    }

    collection.setCssTextAlignment = function(css, h, v) {
        switch (parseInt(h, 10)) {
            case 0:
                css.set("text-align", "left");
                break;
            case 1:
                css.set("text-align", "center");
                break;
            case 2:
                css.set("text-align", "right");
                break;
        }

        switch (parseInt(v, 10)) {
            case 0:
                css.set("vertical-align", "top");
                break;
            case 1:
                css.set("vertical-align", "middle");
                break;
            case 2:
                css.set("vertical-align", "bottom");
                break;
        }
    }

    ]]>
</Script>

<Shape id="button" displayName="Button" icon="Icons/button_standard.png">
    <Properties>
        <PropertyGroup>
            <Property name="box" type="Dimension">60,25</Property>
            <Property name="smartColor" displayName="Automatic border color" type="Bool">true</Property>
            <Property name="radius" displayName="Corner Radius" type="Handle" p:lockY="true" p:minX="0" p:maxX="$box.h / 2" p:noScale="true">4,0</Property>
        </PropertyGroup>
        <PropertyGroup name="Text">
            <Property name="textContent" displayName="Text Content" type="PlainText"><![CDATA[Button]]></Property>
            <Property name="textFont" displayName="Default Font" type="Font"><E>$$textFont</E></Property>
            <Property name="textColor" displayName="Text Color" type="Color"><E>$$textColor</E></Property>
            <Property name="textAlign" displayName="Text Alignment" type="Alignment">1,1</Property>
        </PropertyGroup>
        <PropertyGroup name="Background">
            <Property name="fillColor" displayName="Color" type="Color"><E>$$neutral</E></Property>
        </PropertyGroup>
        <PropertyGroup name="Border">
            <Property name="strokeColor" displayName="Border Color" type="Color"><E>$$strokeColor</E></Property>
            <Property name="strokeStyle" displayName="Border Style" type="StrokeStyle">1|</Property>
        </PropertyGroup>
        <PropertyGroup name="Style">
            <Property name="dropdown" displayName="Dropdown type" type="Enum"
                p:enumValues="['none|None', 'dropdown|Dropdown', 'split|Split dropdown']">none</Property>
            <Property name="dropdownDirection" displayName="Dropdown direction" type="Enum"
                p:enumValues="['1|Down', '-1|Up']">1</Property>
            <Property name="position" displayName="Group position" type="Enum"
                p:enumValues="['none|None', 'left|Left', 'right|Right', 'middle|Middle', 'top|Top', 'bottom|Bottom']">none</Property>
        </PropertyGroup>
    </Properties>
    <Behaviors>
        <For ref="bg">
            <D>
                var tl, tr, br, bl; // Radius of each corner
                var r = $radius.x;
                switch ($position.value) {
                    case "middle":
                        tl = tr = br = bl = 0;
                        break;
                    case "left":
                        tl = bl = r;
                        tr = br = 0;
                        break;
                    case "right":
                        tr = br = r;
                        tl = bl =0;
                        break;
                    case "top":
                        tl = tr = r;
                        bl = br = 0;
                        break;
                    case "bottom":
                        tl = tr = 0;
                        bl = br = r;
                        break;
                    default:
                        tl = tr = br =bl = r;
                        break;
                }

                "M " + tl + ",0 L " + ($box.w - tr) + ",0 a " + tr + "," + tr + " 0 0,1 " + tr + "," + tr
                + "l 0," + ($box.h - tr - br) + " a " + br + "," + br + " 0 0,1 " + -br + "," + br
                + "L " + bl + "," + $box.h + " a " + bl + "," + bl + " 0 0,1 " + -bl + "," + -bl
                + "L 0," + tl + " a " + tl + "," + tl + " 0 0,1 " + tl + "," + -tl + "Z"
            </D>
            <Fill>$fillColor</Fill>
            <StrokeColor>$smartColor.value ? collection.darken($fillColor, 5) : $strokeColor</StrokeColor>
            <StrokeStyle>$strokeStyle</StrokeStyle>
        </For>
        <For ref="text">
            <Font>$textFont</Font>
            <Fill>$textColor</Fill>
            <Color>$textColor</Color>
            <PlainTextContent>
                <Arg>$textContent</Arg>
                <Arg>
                    var width = $box.w;

                    switch ($dropdown.value) {
                        case "split":
                            width = $box.w - 26; break;
                        case "dropdown":
                            width = $box.w - 12; break;
                    }

                    Bound.fromBox(new Dimension(width, $box.h), 12, 6)</Arg>
                <Arg>$textAlign</Arg>
            </PlainTextContent>
        </For>
        <For ref="arrow">
            <Visibility>$dropdown.value != "none"</Visibility>
            <D>"M " + ($box.w - ($dropdown.value == "split" ? 17 : 20)) + "," + (($box.h / 2) + ($dropdownDirection.value * -2)) + " l 8,0 l -4," + ($dropdownDirection.value * 4) + " Z"</D>
            <Fill>$textColor</Fill>
        </For>
        <For ref="split">
            <Visibility>$dropdown.value == "split"</Visibility>
            <D>"M" + ($box.w - 26) + ",0 l 0," + $box.h</D>
            <StrokeColor>$smartColor.value ? collection.darken($fillColor, 5) : $strokeColor</StrokeColor>
            <StrokeStyle>$strokeStyle</StrokeStyle>
        </For>
    </Behaviors>

    <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
        <path id="bg" />
        <text id="text"/>
        <path id="arrow" style="opacity:1;fill:#000000;fill-opacity:1;stroke:none" />
        <path id="split" style="opacity:1;fill:#000000;fill-opacity:1;stroke:none" />
    </p:Content>
</Shape>

